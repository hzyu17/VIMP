<!-- let the alpha here serve as the upper bound of the KL divergence -->
<Commons>
    <total_time>4.25</total_time>
    <n_states>50</n_states>
    <coeff_Qc>1.0</coeff_Qc>
    <GH_deg>6</GH_deg>
    <sig_obs>20.5</sig_obs>
    <map_name>2dpr_map2</map_name>
    <eps_sdf>0.5</eps_sdf>
    <radius>1.0</radius>
    <step_size>2.0</step_size>
    <alpha>30</alpha>
    <init_precision_factor>3000.0</init_precision_factor>
    <boundary_penalties>1000.0</boundary_penalties>
    <temperature>2.5</temperature>
    <high_temperature>15.0</high_temperature>
    <stop_err>1e-5</stop_err>
    <low_temp_iterations>15</low_temp_iterations>
    <max_iterations>20</max_iterations>
    <max_n_backtracking>10</max_n_backtracking>
</Commons>

<Experiment1>
    <start_pos>
        <x>7.0</x>
        <y>-5.0</y>
        <vx>0.0</vx>
        <vy>0.0</vy>
    </start_pos>

    <goal_pos>
        <x>-10.0</x>
        <y>17.0</y>
        <vx>0.0</vx>
        <vy>0.0</vy>
    </goal_pos>

    <saving_prefix>matlab_helpers/ProxKL-examples/2d_pR/sparse_gh/map2/case1/</saving_prefix>

</Experiment1>


