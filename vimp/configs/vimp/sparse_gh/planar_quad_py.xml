<Commons>
    <total_time>12</total_time>
    <n_states>500</n_states>
    <coeff_Qc>1.0</coeff_Qc>
    <GH_deg>6</GH_deg>
    <sig_obs>7</sig_obs>
    <map_name>MultiObstacleLongRangeMap</map_name>
    <eps_sdf>1.0</eps_sdf>
    <radius>0.4</radius>
    <step_size>0.45</step_size>
    <alpha>1.0</alpha>
    <init_precision_factor>10000.0</init_precision_factor>
    <boundary_penalties>5000.0</boundary_penalties>
    <temperature>6000</temperature>
    <high_temperature>20000</high_temperature>
    <stop_err>1e-5</stop_err>
    <low_temp_iterations>25</low_temp_iterations>
    <max_iterations>20</max_iterations>
    <max_n_backtracking>20</max_n_backtracking>
    <max_linear_iter>1</max_linear_iter>
</Commons>

<Experiment1> 
    <start_pos>
        <x>-30.0</x>
        <z>-20.0</z>
        <phi>0.0</phi>
        <vx>0.0</vx>
        <vz>0.0</vz>
        <vphi>0.0</vphi>
    </start_pos>

    <goal_pos>
        <x>210.0</x>
        <z>195.0</z>
        <phi>-0.34906585</phi>
        <vx>1.0</vx>
        <vz>1.5</vz>
        <vphi>0.0</vphi>
    </goal_pos>
    
    <saving_prefix>matlab_helpers/GVIMP-examples/2d_Quad/case2_300_states/</saving_prefix>

</Experiment1>




